#include <iostream>     //Задача 1
using namespace std;
int main() {
	setlocale(LC_ALL, "RU");

	int n;
	cout << "Введите размер квадратной матрицы: ";
	cin >> n;
	int arr[100][100];
	int res1[100][100], res2[100][100], res3[100][100], res4[100][100];

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cin>>arr[i][j];
		}
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			res1[i][j]= arr[j][i];
			res2[j][i] = arr[n-1-i][n-1-j];
			res3[i][j] = arr[i][n-1-j];
			res4[i][j] = arr[n - 1 - i][j];
		}
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout<<arr[i][j]<<" ";
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << res1[i][j] << " ";
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << res2[i][j] << " ";
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << res3[i][j] << " ";
		}
		cout << endl;
	}
	cout << endl;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cout << res4[i][j] << " ";
		}
		cout << endl;
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (res1[i][j] == res2[i][j] && res3[i][j] == res4[i][j] && res1[i][j] == res3[i][j]) {
				cout << "Совпадающие элементы с индексом " << i << " " << j << endl;
			}
		}
	}


	return 0;
}
