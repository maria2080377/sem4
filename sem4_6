#include <iostream>    //Задача 6
using namespace std;

void gaussJordan(double matrix[2][3]) {
    int n = 2;
    for (int i = 0; i < n; i++) {
        double pivot = matrix[i][i];
        for (int j = 0; j <= n; j++) {
            matrix[i][j] /= pivot;
        }
        for (int k = 0; k < n; k++) {
            if (k != i) {
                double factor = matrix[k][i];
                for (int j = 0; j <= n; j++) {
                    matrix[k][j] -= factor * matrix[i][j];
                }
            }
        }
    }
}

int main() {
    double matrix[2][3] = {
        {2, 3, 13},
        {5, -1, 9}
    };

    gaussJordan(matrix);

    for (int i = 0; i < 2; i++) {
        cout << "x" << i + 1 << " = " << matrix[i][2] << endl;
    }

    return 0;
}
